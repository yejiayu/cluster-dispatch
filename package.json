{
  "name": "cluster-dispatch",
  "version": "1.3.0",
  "description": "解决Node.js在Cluster模式下的连接/资源复用问题",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && DEBUG=cluster-dispatch:* egg-bin test",
    "coverage": "npm run cov && cat ./coverage/lcov.info | coveralls",
    "cov": "npm run build && egg-bin cov",
    "build": "./node_modules/.bin/babel src -d lib",
    "watch": "./node_modules/.bin/babel src -w --out-dir lib",
    "example": "npm run build && DEBUG=cluster-dispatch:* node example/dispatch.js",
    "publish-npm": "npm run build && npm publish"
  },
  "author": {
    "name": "叶嘉雨",
    "email": "yejiayu.fe@gmail.com"
  },
  "dependencies": {
    "cfork": "^1.5.1",
    "co": "^4.6.0",
    "debug": "^2.3.0",
    "is-type-of": "^1.0.0",
    "lodash": "^4.16.2",
    "sdk-base": "^2.0.1",
    "socket-messenger": "^0.1.0",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "0.0.6",
    "babel-register": "^6.18.0",
    "debug": "^2.3.3",
    "eslint": "^3.6.1",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.16.0",
    "koa": "^1.2.4",
    "mm": "^2.0.0",
    "power-assert": "^1.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yejiayu/dispatch.git"
  },
  "license": "ISC"
}
